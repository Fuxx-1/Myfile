<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>randomServlet</servlet-name>
    <servlet-class>org.tomcat.servlet.RandomServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>randomServlet</servlet-name>
    <url-pattern>/random</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>randomSuccessServlet</servlet-name>
    <jsp-file>/WEB-INF/random.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>randomSuccessServlet</servlet-name>
    <url-pattern>/random_success</url-pattern>
  </servlet-mapping>

</web-app>
